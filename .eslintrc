{
    "root": true,
    "plugins": [
        "prettier",
        "jest"
    ],
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "rules": {
        "no-console": 1,
        "prettier/prettier": 2
    },
    "env": {
        "browser": true,
        "node": true,
        "jest/globals": true
    },
    "prettier/prettier": [
        "error",
        {
            "endOfLine": "auto"
        }
    ]
}